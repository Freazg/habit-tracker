{
  "name": "habit-tracker",
  "version": "1.0.0",
  "description": "Habit Tracker - Node.js SDLC Project",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:prod": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest unit",
    "test:integration": "jest integration",
    "test:e2e": "jest e2e",
    "test:mutation": "stryker run",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Freazg/habit-tracker.git"
  },
  "keywords": [
    "habit",
    "tracker",
    "nodejs",
    "typescript"
  ],
  "author": "Sas Yevhenii <qwerty2015f5@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Freazg/habit-tracker/issues"
  },
  "homepage": "https://github.com/Freazg/habit-tracker#readme",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^9.3.0",
    "@stryker-mutator/jest-runner": "^9.3.0",
    "@stryker-mutator/typescript-checker": "^9.3.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.2",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.11",
    "prettier": "^3.4.2",
    "prisma": "^6.19.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "bcrypt": "^5.1.1",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^11.0.3",
    "zod": "^3.24.1"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
